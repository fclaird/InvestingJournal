<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spq.turtlebug.investingjournal">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/investing_journal_launcher"
        android:roundIcon="@mipmap/investing_journal_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SEARCH" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEntry"
            android:label="@string/title_activity_add_entry"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        />
        <activity
            android:name=".SearchResult"
            android:label="@string/title_activity_search_result"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>
</manifest>